{
    "name": "pdfask",
    "version": "1.0.0",
    "description": "PdfAsk",
    "main": "index.js",
    "config": {
        "port": 3008,
        "vd": "/"
    },
    "scripts": {
        "start": "npm-run-all -p gulpsass start-imvc",
        "gulpsass": "gulp && gulp watch",
        "start-imvc": "react-imvc start --config ./imvc.config.js",
        "start:debug": "react-imvc start?inspect-brk --config ./imvc.config.js",
        "build": "react-imvc build --config ./imvc.config.js",
        "test": "react-imvc test",
        "rebuild-restart": "pm2 kill && npm run build && pm2 start ./publish/start.js -i 1"
    },
    "dependencies": {
        "@pinecone-database/pinecone": "^0.0.10",
        "body-parser": "^1.20.2",
        "body-parser-xml": "^2.0.3",
        "bootstrap": "^5.2.3",
        "clipboard-copy": "^4.0.1",
        "cross-fetch": "3.1.5",
        "dayjs": "1.11.7",
        "dotenv": "^16.0.3",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-postcss": "^9.0.1",
        "lodash": "4.17.21",
        "openai": "^3.2.1",
        "react": "^17.0.2",
        "react-bootstrap": "^2.7.2",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "^17.0.2",
        "react-imvc": "^3.2.7",
        "react-pdf": "^6.2.2",
        "react-stickynode": "^4.1.0"
    },
    "devDependencies": {
        "@types/lodash": "4.14.191",
        "css-loader": "^6.7.3",
        "esm": "^3.2.25",
        "gulp": "^4.0.2",
        "gulp-sass": "^5.1.0",
        "node-sass": "^8.0.0",
        "npm-run-all": "^4.1.5",
        "pdf-loader": "^1.0.2",
        "style-loader": "^3.3.1",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^9.0.0",
        "typescript": "^4.9.5"
    }
}
